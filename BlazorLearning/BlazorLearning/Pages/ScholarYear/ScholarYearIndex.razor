@page "/scholaryear"
@inject Data.ApplicationDbContext context

<h3>Scholar Years</h3>

<div class="row">
    <div class="d-flex">
        <a class="btn btn-outline-primary" href="/scholaryear/create"> Create</a>
    </div>
</div>
<table class="table table-hover">
    <thead>
        <tr>
            <td>Name</td>
            <td>Created Date</td>
            <td></td>
        </tr>
    </thead>
    <tbody>
    @foreach (var year in SchoolYearsData)
    {
        <ScholarYearComponent1 Data=@year />
    }
    </tbody>
</table>
@code {
    internal IEnumerable<Data.ScholarYear> SchoolYearsData;

    protected override void OnInitialized()
    {
        SchoolYearsData = context.ScholarYears;
    }
}
